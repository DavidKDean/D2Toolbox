{
	"name": "d2toolbox",
	"version": "1.1.0",
	"description": "Multiple tools to make playing Diablo 2 easier.",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --win portable"
	},
	"build": {
		"appId": "com.electron.d2toolbox",
		"productName": "Diablo 2 Toolbox",
		"win": {
			"target": "nsis",
			"icon": "img/icon.ico"
		}
	},
	"repository": "https://github.com/DavidKDean/D3Toolbox",
	"author": "David Kirby Dean",
	"license": "UNLICENSED",
	"devDependencies": {
		"electron": "^18.2.0",
		"electron-builder": "^23.0.3"
	},
	"dependencies": {
		"yarn": "^1.22.18"
	}
}
